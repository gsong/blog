    "gatsby-plugin-theme-ui": "^0.3.4",
    "gatsby-plugin-typography": "^2.7.0",
    "gatsby-remark-autolink-headers": "^2.6.0",
    "gatsby-remark-copy-linked-files": "^2.5.0",
    "gatsby-remark-prismjs": "^3.8.0",
