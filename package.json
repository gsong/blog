    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "theme-ui": "^0.3.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "@svgr/cli": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.21.5",
    "gatsby-plugin-exclude": "^1.0.2",
