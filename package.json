{
  "name": "gsong-personal-site",
  "description": "George's Personal Website",
  "version": "1.0.0",
  "author": "George Song <george@damacy.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gsong/personal-site"
  },
  "scripts": {
    "build": "rimraf public && gatsby build",
    "deploy": "npm run build && gh-pages -d public",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"src/**/*.js\"",
    "init-dev": "npx pnpm i",
    "lint": "eslint . && stylelint . && remark -f -e md,mdx .",
    "start": "npm run develop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "website"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "gatsby": "^2.23.22",
    "gatsby-image": "^2.4.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-react": "^7.20.3",
    "gatsby-plugin-emotion": "^4.3.9",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-mdx": "^1.2.22",
    "gatsby-plugin-offline": "^3.2.16",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-sharp": "^2.6.18",
    "gatsby-remark-prismjs": "^3.5.9",
    "gatsby-remark-smartypants": "^2.3.9",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-sharp": "^2.5.10",
    "gh-pages": "^3.1.0",
    "graphql": "^15.3.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.1",
    "remark-lint-no-dead-urls": "^1.0.2",
    "remark-validate-links": "^10.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  }
}
