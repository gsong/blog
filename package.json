    "react-helmet": "^6.1.0",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.2",
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@svgr/cli": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "commitlint": "^9.1.2",
