{
  "name": "gsong-personal-site",
  "description": "George's Personal Website",
  "version": "1.0.0",
  "author": "George Song <george@damacy.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gsong/personal-site"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "deploy": "npm run build && gh-pages -d public",
    "format": "prettier --write \"src/**/*.js\"",
    "init-dev": "npx pnpm i",
    "lint": "eslint . && stylelint . && remark -f -e md,mdx .",
    "serve": "npm run build && gatsby serve -H 0.0.0.0",
    "start": "gatsby clean && gatsby develop -H 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "website"
  ],
  "dependencies": {
